---
import Layout from '../layouts/Layout.astro';

const classes = [
	{ id: 1, name: 'Mathematics Grade 10' },
	{ id: 2, name: 'History Grade 11' },
	{ id: 3, name: 'Science Grade 9' },
];
---

<Layout title="Classes">
	<h1>Classes</h1>
	<a href="/classes/new">Add New Class</a>
	<ul>
		{classes.map(c => (
			<li>
				<a href={`/classes/${c.id}`}>{c.name}</a>
			</li>
		))}
	</ul>
</Layout>
